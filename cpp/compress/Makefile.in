# Makefile to compile the module in this directory

SOURCES = $(wildcard db/compress/*.cpp) \
          $(wildcard db/compress/deflate/*.cpp) \
          $(wildcard db/compress/gzip/*.cpp) \
          $(wildcard db/compress/zip/*.cpp)

LIBRARIES = dbcompress
DYNAMIC_LINK_LIBRARIES = dbrt dbio dbutil

DYNAMIC_WINDOWS_LIBRARIES = mgwz
STATIC_WINDOWS_LIBRARIES = z

DYNAMIC_MACOS_LIBRARIES = z

# ----------- Standard Makefile
include @DBCOREDIR@/setup/Makefile.base
