# Makefile to compile the module in this directory

LIBRARIES = mocompress
mocompress_HEADERS = \
	$(wildcard *.h) \
	$(wildcard deflate/*.h) \
	$(wildcard gzip/*.h) \
	$(wildcard zip/*.h)
mocompress_SOURCES = \
	$(wildcard *.cpp) \
	$(wildcard deflate/*.cpp) \
	$(wildcard gzip/*.cpp) \
	$(wildcard zip/*.cpp)

DYNAMIC_LINK_LIBRARIES = mort moio moutil

DYNAMIC_LINUX_LINK_LIBRARIES = z
DYNAMIC_MACOS_LINK_LIBRARIES = z
DYNAMIC_WINDOWS_LINK_LIBRARIES = mgwz

# ----------- Standard Makefile
include @MONARCH_DIR@/setup/Makefile.base
