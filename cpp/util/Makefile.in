# Makefile to compile the module in this directory

LIBRARIES = moutil
moutil_HEADERS = \
	$(wildcard *.h) \
	$(wildcard regex/*.h)
moutil_SOURCES = \
	$(wildcard *.cpp) \
	$(wildcard regex/*.cpp)

DYNAMIC_LINK_LIBRARIES += mort

ifeq (@BUILD_FOR_WINDOWS@,yes)
DYNAMIC_LINK_LIBRARIES += libgnurx-0
endif

# ----------- Standard Makefile
include @MONARCH_DIR@/setup/Makefile.base

